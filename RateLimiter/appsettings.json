{
    "AppName": "RateLimiter",
    "Enabled": true,
    "Header": "X-ApiKey",
    "AllowList": [
        "real-client"
    ],
    "BlockList": [
        "fake-client"
    ],
    "Rules": [
        {
            "Strategy": "Header",
            "Guard": "US",
            "Policy": "RequestsPerTimeSpan",
            "RequestCount": 50,
            "LimitInSeconds": 1,
            "TimeSinceLastRequestInSeconds": 1
        },
        {
            "Strategy": "Header",
            "Guard": "EU",
            "Policy": "TimeSpanSinceLastRequest",
            "RequestCount": 50,
            "LimitInSeconds": 2,
            "TimeSinceLastRequestInSeconds": 2
        },
        {
            "Strategy": "Endpoint",
            "Guard": "api/users",
            "Policy": "TimeSpanSinceLastRequest",
            "RequestCount": 50,
            "LimitInSeconds": 1,
            "TimeSinceLastRequestInSeconds": 2
        },
        {
            "Strategy": "Endpoint",
            "Guard": "api/bid",
            "Policy": "TimeSpanSinceLastRequest",
            "RequestCount": 5,
            "LimitInSeconds": 1,
            "TimeSinceLastRequestInSeconds": 2
        }
    ]
}
