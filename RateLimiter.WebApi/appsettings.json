{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RequestLimiterPolicy": {
    "Amount": "5",
    "TimeSpan": "15000" // 15s.
  },
  "FixedCapacityByCountryPolicy": {
    "Countries": [
      {
        "Name": "Default",
        "Amount": "2",
        "TimeSpan": "15000" // 15s.
      },
      {
        "Name": "US",
        "Amount": "2",
        "TimeSpan": "15000" // 15s.
      },
      {
        "Name": "EU",
        "Amount": "3",
        "TimeSpan": "15000" // 15s.
      }
    ]
  },
  "RequestLimiterValue": "3000" // 3s.
}
