{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LimiterOptions": {
    "LocationLimiters": [
      {
        "LocationName": "EU",
        "TimeRange": "00:00:15",
        "AllowedRequestsCountPerTimeRange": 2
      },
      {
        "LocationName": "USA",
        "TimeRange": "00:01:00",
        "AllowedRequestsCountPerTimeRange": 200
      }
    ]
  }
}
